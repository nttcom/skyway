{
  "name": "SkyWay",
  "author": "Ewan Leaver",
  "version": "0.1.1",
  "description": "SkyWay Website",
  "repository": {
    "type": "git",
    "url": "git://github.com/nttcom-webcore/skyway-renewal.git"
  },
  "scripts": {},
  "browser": {
    "underscore": "backbone/node_modules/underscore",
    "bootstrap": "./node_modules/bootstrap/dist/js/bootstrap.min.js",
    "plugin": "./src/javascript/vendor/jquery-plugin.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify",
      "hbsfy"
    ]
  },
  "browserify-shim": {
    "bootstrap": {
      "exports": "bootstrap",
      "depends": [
        "jquery:jQuery"
      ]
    },
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    }
  },
  "devDependencies": {
