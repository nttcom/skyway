    "gulp-autoprefixer": "^2.3.1",
    "gulp-changed": "^1.1.1",
    "gulp-compass": "^2.1.0",
    "gulp-concat": "*",
    "gulp-filesize": "0.0.6",
    "gulp-htmlmin": "*",
    "gulp-iconfont": "^1.0.0",
    "gulp-ignore": "*",
    "gulp-imagemin": "^2.2.1",
    "gulp-jekyll": "*",
    "gulp-jshint": "*",
    "gulp-kramdown": "*",
    "gulp-load-plugins": "*",
    "gulp-minify-css": "~0.5.1",
